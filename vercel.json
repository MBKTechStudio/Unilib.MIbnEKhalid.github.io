{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "Assets/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*).html",
      "dest": "/$1.html"
    },
    {
      "src": "/Assets/(.*)",
      "dest": "/Assets/$1"
    },
    {
      "src": "/read-file",
      "dest": "/index.js"
    },
    {
      "src": "/add",
      "dest": "/index.js"
    },
    {
      "src": "/history",
      "dest": "/history/index.html"
    },
    {
      "src": "/.*",
      "dest": "/404.html"
    }
  ]
}
