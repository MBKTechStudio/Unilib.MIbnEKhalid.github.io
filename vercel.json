{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "Assets/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/Assets/(.*)",
        "dest": "/Assets/$1"
      },
      {
        "src": "/history/(.*)",
        "dest": "/history/index.html"
      },
      {
        "src": "/add",
        "dest": "/ui.html"
      },
      {
        "src": "/",
        "dest": "/index.html"
      }
    ]
  }
  