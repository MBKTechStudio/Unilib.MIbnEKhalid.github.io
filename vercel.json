{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/add",
      "methods": ["GET"],
      "dest": "/server.js"
    },
    {
      "src": "/Add",
      "methods": ["GET"],
      "dest": "/server.js"
    },
    {
      "src": "/read-file",
      "methods": ["GET"],
      "dest": "/server.js"
    },
    {
      "src": "/append-file",
      "methods": ["POST"],
      "dest": "/server.js"
    },
    {
      "src": "/history",
      "methods": ["GET"],
      "dest": "/server.js"
    }
  ]
}
